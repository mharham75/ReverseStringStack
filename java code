import java.io.*;
import java.util.*;
import java.lang.*;
public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine();
		Stack<Character> st = new Stack();
		for(int i=0; i<s.length(); i++)
		{
		   st.push(s.charAt(i));
		}
		
		while(!st.isEmpty())
		    System.out.print(st.pop());
	}
}
